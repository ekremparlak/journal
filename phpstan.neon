parameters:
    bootstrapFiles:
        - .phpstan-bootstrap.php
    level: 5
    paths:
        - private/lib
        - private/scripts
    excludePaths:
        - private/cache
        - private/templates
        - tests
        - vendor
    dynamicConstantNames:
        - BASE_URL
        - DEBUG_MODE
        - SSL_IS_ENABLED
        - SITE_TITLE
        - ADMIN_EMAIL_ADDRESS
        - SENTRY_ENABLED